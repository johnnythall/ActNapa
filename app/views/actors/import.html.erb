<%- model_class = Actor -%>
<div class="page-header">
  <h1><%=t '.title', :default => [:'helpers.titles.new', 'Import %{model}'], :model => model_class.model_name.human.titleize %></h1>
</div>

<%= form_tag home_path, multipart: true do %>
  <%= file_field_tag :file %>
  <%= submit_tag "Import CSV" %>
<% end %>

